  hal_id = {inria-00456167},
  isbn = {978-1-57735-449-9},
  keywords = {Evolutionary Computation, Automated Planning, Divide-and-Evolve},
  language = {Anglais},
  pdf = {http://hal.inria.fr/inria-00456167/PDF/icaps10.pdf},
  url = {http://hal.inria.fr/inria-00456167}
}

@INPROCEEDINGS{Bibai2009,
  author = {Biba{\"i}, Jacques and Sav{\'e}ant, Pierre and Schoenauer, Marc and
	Vidal, Vincent},
  title = {{Learning Divide-and-Evolve Parameter Configurations with Racing}},
  booktitle = {{ICAPS-09-Workshop on Planning and Learning}},
  year = {2009},
  editor = {AAAI press},
  address = {Thessaloniki, Gr{\`e}ce},
  organization = {ICAPS and University of Macedonia},
  publisher = {AAAI press},
  abstract = {{The sub-optimal DAE planner implements the stochastic approach for
	domain-independent planning decomposition introduced in (Schoenauer,
	Sav{\'e}ant, and Vidal 2006; 2007). This planner optimizes either
	the makespan or the number of actions by generating ordered sequences
	of intermediate goals via a process of artificial evolution. The
	evolutionary part of DAE uses the Evolving Objects (EO) library,
	and the embedded planner it is based on is the non-optimal STRIPS
	planner YAHSP (Vidal 2004). For a given domain, the learning phase
	uses a racing procedure to choose the rates of the different variation
	operators used in DAE, and processes the results obtained during
	this process to specify the predicates that will be later used to
	describe the intermediate goals.}},
  affiliation = {Thales Research and Technology [Palaiseau] - TRT , TAO - INRIA Saclay
	- Ile de France , Centre de Recherche en Informatique de Lens - CRIL},
